      <portlet-style>exo-content</portlet-style>
      <title>Community</title>
      <windowId>demo:/content/DisplayContent/community</windowId>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>title=Default Content</value>
          <value>uri=/community/charter</value>
          <value>encoding=UTF-8</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page>

  <page>
    <owner>demo</owner>
    <name>/home/community/forum</name>
