          </preference>
          <preference>
            <name>channel</name>
            <value>News</value>
          </preference>
          <preference>
            <name>item-to-show</name>
            <value>5</value>
          </preference>
        </portlet-preferences>
      </portlet>
      <container id="blogs" renderer="TabRenderer">  
        <container id="Portals" renderer="TabRenderer">  
