      <windowId>admin:/content/JCRExplorer/jcr</windowId>
    </portlet>
  </page>

  <page>
    <owner>admin</owner>
    <name>/error/page/page-not-found</name>
    <title>${page-not-found}</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <title>Page Not Found</title>
      <windowId>admin:/content/DisplayContent/page-not-found</windowId>
    </portlet>
  </page>

  <page>
    <owner>admin</owner>
    <name>/error/page/user-page-view-permission</name>
