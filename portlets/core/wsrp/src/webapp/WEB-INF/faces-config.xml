<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
"http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>
  <factory>
    <faces-context-factory>org.exoplatform.portlet.faces.context.FacesPortletContextFactoryImpl</faces-context-factory>
    <lifecycle-factory>org.exoplatform.portlet.faces.lifecycle.ExoLifecycleFactory</lifecycle-factory>
  </factory>

  <render-kit>
    <renderer>
      <component-family>org.exoplatform.portlets.wsrp.component.UIProducerNode</component-family>
      <renderer-type>ProducerNodeRenderer</renderer-type>
      <renderer-class>org.exoplatform.portlets.wsrp.renderer.html.ProducerNodeRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>org.exoplatform.portlets.wsrp.component.UIProducerMenu</component-family>
      <renderer-type>ProducerMenuRenderer</renderer-type>
      <renderer-class>org.exoplatform.portlets.wsrp.renderer.html.ProducerMenuRenderer</renderer-class>
    </renderer>
    <renderer>
      <component-family>org.exoplatform.portlets.wsrp.component.UIProducerInfo</component-family>
      <renderer-type>ProducerInfoRenderer</renderer-type>
      <renderer-class>org.exoplatform.portlets.wsrp.renderer.html.ProducerInfoRenderer</renderer-class>
    </renderer>
  </render-kit>
</faces-config>
