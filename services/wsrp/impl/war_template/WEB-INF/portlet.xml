<?xml version="1.0" encoding="UTF-8"?>

<portlet-app version="1.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

  <portlet>
    <description xml:lang="EN">Usual Hello World Portlet</description>
    <description xml:lang="FR">Salut le monde Portlet</description>
    <portlet-name>HelloWorld</portlet-name>
    <display-name xml:lang="EN">HelloWorldPortlet</display-name>
    <display-name xml:lang="FR">SalutLeMondePortlet</display-name>
    <portlet-class>org.exoplatform.services.portletcontainer.test.portlet.WSRPHelloWorldPortlet</portlet-class>
    <init-param>
      <description>something to describe</description>
      <name>initName</name>
      <value>initValue</value>
    </init-param>
    <expiration-cache>4</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>config</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supports>
      <mime-type>text/wml</mime-type>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>EN</supported-locale>
    <resource-bundle>org.exoplatform.services.portletcontainer.test.portlet.resources</resource-bundle>
    <portlet-info>
      <title>HelloWorld title</title>
    </portlet-info>
    <portlet-preferences>
      <preference>
        <name>time-server</name>
        <value>http://timeserver.myco.com</value>
      </preference>
      <preference>
        <name>port</name>
        <value>404</value>
        <read-only>true</read-only>
      </preference>
      <preference>
        <name>time-format</name>
        <value>HH</value>
        <value>mm</value>
        <value>ss</value>
        <read-only>false</read-only>
      </preference>
    </portlet-preferences>
    <security-role-ref>
      <role-name>trustedUser</role-name>
      <role-link>auth-user</role-link>
    </security-role-ref>
  </portlet>

  <portlet>
    <description xml:lang="EN">Usual Hello World Portlet</description>
    <description xml:lang="FR">Salut le monde Portlet</description>
    <portlet-name>HelloWorld2</portlet-name>
    <display-name xml:lang="EN">HelloWorldPortlet</display-name>
    <display-name xml:lang="FR">SalutLeMondePortlet</display-name>
    <portlet-class>org.exoplatform.services.portletcontainer.test.portlet.HelloWorld2</portlet-class>
    <expiration-cache>4</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>config</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>EN</supported-locale>
    <portlet-info>
      <title>HelloWorld title</title>
    </portlet-info>
  </portlet>	

