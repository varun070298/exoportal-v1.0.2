  <dependencies>
    <dependency>
      <groupId>exo</groupId>
      <artifactId>exoplatform.commons</artifactId>
      <version>${exoplatform.version}</version>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>

    <dependency>
      <groupId>axis</groupId>
      <artifactId>axis</artifactId>
      <version>1.1</version>
    </dependency>

    <dependency>
      <groupId>wsdl4j</groupId>
